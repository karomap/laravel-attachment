{
    "name": "karomap/laravel-attachment",
    "description": "Attachment for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "attachment"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/support": "^11.0",
        "intervention/image-laravel": "^1.3",
        "spatie/laravel-activitylog": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Karomap\\LaravelAttachment\\": "src/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Karomap\\LaravelAttachment\\AttachmentServiceProvider"
            ]
        }
    }
}
