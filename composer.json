{
    "name": "karomap/laravel-attachment",
    "description": "Attachment for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "attachment"
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "spatie/laravel-activitylog": "^4.0",
        "intervention/image": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Karomap\\LaravelAttachment\\": "src/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Karomap\\LaravelAttachment\\AttachmentServiceProvider"
            ]
        }
    }
}
